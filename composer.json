{
    "name": "emandiev/regex-replace-twig-filter",
    "description": "Adds PHP's preg_replace function as a Twig filter.",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "regex replace twig filter"
    ],
    "support": {
        "docs": "https://github.com/emandiev/regex-replace-twig-filter/blob/master/README.md",
        "issues": "https://github.com/emandiev/regex-replace-twig-filter/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Danail Emandiev",
            "homepage": "https://emandiev.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "emandiev\\regexreplacetwigfilter\\": "src/"
        }
    },
    "extra": {
        "name": "Regex Replace Twig Filter",
        "handle": "regex-replace-twig-filter",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/emandiev/regex-replace-twig-filter/master/CHANGELOG.md",
        "class": "emandiev\\regexreplacetwigfilter\\RegexReplaceTwigFilter"
    }
}
